    "adonis-app"
  ],
  "dependencies": {
    "@adonisjs/ace": "^11.3.1",
    "@adonisjs/ally": "^2.2.0",
    "@adonisjs/auth": "^3.2.0",
    "@adonisjs/bodyparser": "^2.3.0",
    "@adonisjs/cors": "^1.0.7",
    "@adonisjs/fold": "^4.1.0",
    "@adonisjs/framework": "^5.0.13",
    "@adonisjs/ignitor": "^2.0.8",
    "@adonisjs/lucid": "^18.4.2",
    "@adonisjs/validator": "^5.1.0",
    "country-list": "^2.3.0",
    "csv-write-stream": "^2.0.0",
    "cubejs": "^1.3.2",
    "mysql": "^2.18.1"
  },
  "devDependencies": {
    "nodemon": "2.0.1"
  },
  "autoload": {
    "App": "./app"
